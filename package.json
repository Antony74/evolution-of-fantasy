{
    "name": "evolution-of-fantasy",
    "version": "1.0.0",
    "scripts": {
        "clean": "rimraf dist",
        "run": "node dist/index.js",
        "build": "vite build",
        "build-types": "tsc src/index.ts --declaration -emitDeclarationOnly --module commonjs --esModuleInterop true --allowSyntheticDefaultImports true --outFile dist/index.d.ts",
        "prettier": "npx prettier --write .",
        "start": "npm run clean && npm run build && npm run run"
    },
