    "license": "MIT",
    "dependencies": {
        "@maxgraph/core": "^0.16.0",
        "@resvg/resvg-js": "^2.6.2",
        "jsdom": "^26.1.0",
        "jsdom-global": "^3.0.2",
        "mime-types": "^3.0.1",
        "sharp": "^0.34.1"
    },
    "devDependencies": {
        "@types/jsdom": "^21.1.7",
        "@types/jsdom-global": "^3.0.7",
        "@types/mime-types": "^2.1.4",
        "prettier": "^3.5.3",
        "rimraf": "^6.0.1",
        "rollup-plugin-dts": "^6.2.1",
        "vite": "^6.3.2"
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": true
    }
